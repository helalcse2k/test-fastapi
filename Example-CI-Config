steps:
  - uses: actions/checkout@v2
  - name: Set up python 3.9
    uses: actions/setup-python@v2
    with:
      python-version: "3.9"
  - name: Update pip
    run: Python -m pip install --upgrade pip
  - name: install dependencies
    run: pip install -r requirements.txt
  - name: Test with Pytest
    run:  |
      pip install Pytest
      pytest
       